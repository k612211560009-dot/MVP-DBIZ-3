{
  "rewrites": [
    {
      "source": "/auth/:path*",
      "destination": "https://mvp-dbiz-3.onrender.com/api/auth/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "https://mvp-dbiz-3.onrender.com/api/:path*"
    },
    {
      "source": "/:path((?!.*\\.).*)",
      "destination": "/index.html"
    }
  ]
}
